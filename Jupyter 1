    "# Город one hot encoding\n",
    "cities_one_hot = pd.get_dummies(dataset['Город'], drop_first=True, prefix='Город')\n",
    "dataset = dataset.drop('Город', axis=1)\n",
    "dataset = dataset.join(cities_one_hot)\n",
    "dataset.head()"
