   "source": [
    "# creating the mdoel\n",
    "model = keras.Sequential([\n",
    "    layers.Dense(50, activation=tf.nn.relu, input_shape=[len(train_dataset.keys())]),\n",
    "    layers.Dense(50, activation=tf.nn.relu),\n",
    "    layers.Dense(50, activation=tf.nn.relu),\n",
    "    layers.Dense(1, activation='linear')\n",
    "])\n",
    "\n",
    "model.compile(\n",
    "    optimizer=keras.optimizers.Adam(learning_rate=10),\n",
    "    loss='mean_squared_error',\n",
    "    metrics=['mean_squared_error', 'mean_absolute_error']\n",
    ")\n",
    "\n",
    "history = model.fit(\n",
    "    x=train_dataset,\n",
    "    y=train_labels,\n",
    "    epochs=10,\n",
    "    validation_split=0.2,\n",
    "    batch_size=1, \n",
    "    verbose=1,\n",
    ")\n",
    "\n",
    "history_set = pd.DataFrame(history.history)\n",
    "history_set['epoch'] = history.epoch\n",
    "history_set.tail()"
   ]
  },
