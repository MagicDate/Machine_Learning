    "# Должность one hot encoding\n",
    "position_one_hot = pd.get_dummies(dataset['Должность'], prefix='Должность', dtype='float')\n",
    "dataset = dataset.drop('Должность', axis=1)\n",
    "dataset = dataset.join(position_one_hot)\n",
    "dataset.head()"
   ]
